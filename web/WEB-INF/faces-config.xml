<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
    <managed-bean>
        <managed-bean-name>DtsBean</managed-bean-name>
        <managed-bean-class>com.beans.DtsBean</managed-bean-class>
    </managed-bean>
    
    
    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
       
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        
         <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
    
    
     <managed-bean>
        <managed-bean-name>DuyuruBean</managed-bean-name>
        <managed-bean-class>com.beans.DuyuruBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
      <managed-bean>
        <managed-bean-name>DavaBean</managed-bean-name>
        <managed-bean-class>com.beans.DavaBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    
    <managed-bean>
        <managed-bean-name>duyuruListeleme</managed-bean-name>
        <managed-bean-class>com.beans.DuyuruListeleme</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>newJSFManagedBean</managed-bean-name>
        <managed-bean-class>com.beans.NewJSFManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
</faces-config>
